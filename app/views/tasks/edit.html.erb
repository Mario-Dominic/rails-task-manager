<%= link_to 'Edit a Task', tasks_path %>

<%= form_with model: @task do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :details %>
  <%= f.text_field :details %>
  <%= f.label :completed, "Completed?" %>
  <%= f.check_box :completed %>
  <%= f.submit "Update Task" %>
<% end %>
